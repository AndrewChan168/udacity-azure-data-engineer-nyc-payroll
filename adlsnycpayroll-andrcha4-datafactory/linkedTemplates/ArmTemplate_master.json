{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adlsnycpayroll-andrcha4-datafactory"
		},
		"adlsnycpayrollDataLakeStg_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsnycpayrollDataLakeStg'"
		},
		"adlsnycpayrollSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adlsnycpayrollSQLDB'"
		},
		"adlsnycpayrollSynapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adlsnycpayrollSynapse'"
		},
		"adlsnycpayrollDataLakeStg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollandrcha4.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adlsnycpayroll-andrcha4-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsnycpayrollDataLakeStg_accountKey": {
						"value": "[parameters('adlsnycpayrollDataLakeStg_accountKey')]"
					},
					"adlsnycpayrollSQLDB_connectionString": {
						"value": "[parameters('adlsnycpayrollSQLDB_connectionString')]"
					},
					"adlsnycpayrollSynapse_connectionString": {
						"value": "[parameters('adlsnycpayrollSynapse_connectionString')]"
					},
					"adlsnycpayrollDataLakeStg_properties_typeProperties_url": {
						"value": "[parameters('adlsnycpayrollDataLakeStg_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adlsnycpayroll-andrcha4-datafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adlsnycpayroll-andrcha4-datafactory_ArmTemplate_0')]"
			]
		}
	]
}