{
	"name": "TitleDataFlow_csvToSynapse",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TitleMastercsv",
						"type": "DatasetReference"
					},
					"name": "TitleCSVSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TitleMDSynapse",
						"type": "DatasetReference"
					},
					"name": "TitleSynapseSink"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> TitleCSVSource",
				"TitleCSVSource sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     staged: true,",
				"     allowCopyCommand: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> TitleSynapseSink"
			]
		}
	}
}